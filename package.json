{
  "name": "tweedle",
  "version": "0.0.0",
  "description": "Open API for downloading all media of a Twitter profile.",
  "main": "./dist/index.js",
  "files": [
    "./dist"
  ],
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "rm --recursive --force ./dist && tsc",
    "build-watch": "tsc --watch",
    "lint": "eslint --ext .ts ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfeigl/tweedle.git"
  },
  "keywords": [
    "open",
    "api",
    "twitter",
    "media",
    "download"
  ],
  "author": "Luca Feigl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lfeigl/tweedle/issues"
  },
  "homepage": "https://github.com/lfeigl/tweedle#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^3.1.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
